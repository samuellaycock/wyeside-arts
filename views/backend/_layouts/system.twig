        <div id="logo">
            <img src="/img/wyeside-logo.png" alt="Wyeside Arts" title="Wyeside Arts" >
        </div>
        <div id="navigation">
            <ul>
                <li><a href="/system/events">Events</a></li>
                <li><a href="/system/blog">Blog</a></li>
                <li><a href="/system/users">Users</a></li>
            </ul>
        </div>
    </div>
    <div id="content">
        {% block content %}{% endblock %}
    </div>
    <div id="footer">

    </div>
</div>
</body>
</html>