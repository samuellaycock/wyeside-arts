  "keywords": [
    "Wyeside",
    "Arts",
    "Center"
  ],
  "config": {
    "github-oauth": {
      "github.com": "d3e179fd8ecd48249586196f05ffb7432f912681"
    },
    "vendor-dir": "_vendor"
  },
  "autoload": {
